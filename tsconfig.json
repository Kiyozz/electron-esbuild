{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "noImplicitReturns": false,
    "noEmitOnError": true,
    "incremental": true,
    "declaration": false,
    "baseUrl": "./",
    "outDir": "dist"
  },
  "include": ["src"]
}
