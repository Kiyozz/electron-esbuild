    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-dev-server": "^4.1.0",
    "autoprefixer": "^10.3.5",
