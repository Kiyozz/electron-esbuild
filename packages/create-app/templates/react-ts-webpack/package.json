  "build": {
    "appId": "io.comp.myapp",
    "productName": "My App",
    "extraMetadata": {
      "name": "My App",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "win": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  }
